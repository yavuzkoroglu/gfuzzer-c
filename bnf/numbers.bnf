; A number is either 0 or a non-zero followed by a sequence of digits
<number>    ::= <zero> | <nonzero> <digit-seq>

; A digit sequence is either one digit or a digit followed by another digit sequence.
<digit-seq> ::= <digit> | <digit> <digit-seq>

; A digit is either zero or non-zero.
<digit>     ::= <zero> | <nonzero>

; There are nine distinct non-zero digits.
<nonzero>   ::= '1' | '2' | '3' | '4' | '5' | '6' | '7' | '8' | '9'
<zero>      ::= '0'
